  completed       = {2011-11-23},
  country         = {United States},
  doi             = {10.1089/bsp.2011.0007},
  investigator    = {Baker, Michael G and Biedrzycki, Paul A and Cowling, Benjamin J and De Angelis, Daniela and Eagle, Nathan and Fine, Annie D and Fraser, Christophe and Hatchett, Richard J and Kohl, Katrin S and Korch, George and Madoff, Lawrence C and Olson, Donald R and Riley, Steven and Simonsen, Lone and Van Kerkhove, Maria D and van Noort, Sander and Viboud, Cécile and Wallinga, Jacco and White, Laura F and Widdowson, Marc-Alain},
  issn-linking    = {1538-7135},
